
@using DicomApp.CommonDefinitions.DTO.AdvertisementDTOs;
@model List<AdsDTO>
@if (Model != null && Model.Count > 0)
{
    foreach (var item in Model)
    {
        <tr id="@item.AdvertisementId" data-Vendor="0">
            <td><input type="checkbox" value="@item.AdvertisementId" id="checkbox_@item.AdvertisementId" name="AdvertisementIds" checked=@item.SELECTED></td>
            <td>@item.RefId</td>
            <td>0</td>
            <td>0</td>
            <td>0</td>
            <td>0 KG</td>
        </tr>
    }
}
else
{
    <tr>
        <td colspan="6" class="text-danger text-center"><h3>No data available in table</h3></td>
    </tr>
}

<input type="hidden" value="@(Model != null ? Model.Count:0)" id="DataCount" />
