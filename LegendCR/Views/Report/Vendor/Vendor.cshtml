<main>
    <div class="container-fluid site-width">
        <div class="row Red-Row">
            <div class="col-12 col-lg-12 col-xl-12 col-sm-12">
                <div class="sub-header">
                    <span class="Header-Titel">Vendor Report</span>
                </div>
            </div>
        </div>
    </div>
    <div class="col-12">
        <span id="Ajaxloader" class="Ajaxloader d-none"></span>
    </div>
    <div class="Table-container">
        <table class="table" id="Table">
            <thead>
                <tr>
                    <td class="text-left">Vendor</td>
                    <td class="text-center">All Orders</td>
                    <td class="text-center">Current</td>
                    <td class="text-center">Done</td>
                    <td class="text-center">Cancelled</td>
                    <td class="text-center">Success %</td>
                    <td class="text-center">RED Fees</td>
                    <td class="text-center">Total</td>
                    <td class="text-center">Balance</td>
                </tr>
            </thead>
            <tbody>
                @if (Model != null)
                {
                    foreach (var vendor in Model)
                    {
                        <tr>
                            <td class="td-medium text-left font-weight-bold" style="font-size: 14px;">@vendor.VendorName</td>

                            <td class="td-medium text-center text-nowrap">
                                <span data-toggle="modal" data-target="#AllShips-Modal_@vendor.VendorId"
                              class="badge badge-pill badge-light font-weight-bold" style="font-size: 16px;">@vendor.TotalShipsCount</span>

                                <!-- START : All Shipments -->
